    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "8.56.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "postcss": "^8.4.32",
